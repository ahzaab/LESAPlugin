#include "include\version.h"
#include <winres.h>

1 VERSIONINFO
 FILEVERSION LESA_VERSION_MAJOR, LESA_VERSION_MINOR, LESA_VERSION_PATCH, LESA_VERSION_BETA
 PRODUCTVERSION LESA_VERSION_MAJOR, LESA_VERSION_MINOR, LESA_VERSION_PATCH, LESA_VERSION_BETA
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "FileDescription", "LESA Plugin"
			VALUE "FileVersion", LESA_VERSION_VERSTRING
            VALUE "InternalName", "LESA Plugin"
            VALUE "LegalCopyright", "MIT License"
            VALUE "ProductName", "LESA Plugin"
			VALUE "ProductVersion", LESA_VERSION_VERSTRING
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
